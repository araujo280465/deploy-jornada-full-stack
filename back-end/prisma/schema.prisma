

generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "sqlite"
  url      = "file:./database.sqlite"

}

model produto {

  id Int @id @default(autoincrement())
  titulo String
  preco Float
  precoDesconto Float
  precoParcelado Float
  caracteristicas String
  imagens String
  estoque Int
  freteGratis Boolean
  full Boolean
}

model pedido {
  id Int @id @default(autoincrement())
  valorTotal Float
  itensVenda String
}